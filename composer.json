{
    "name": "aboodma/gosell-sdk-php-8",
    "description": "Tap Payemnt PHP",
    "version": "0.0.1",
    "type": "library",
    
    "require": {
        "php": ">=8.0",
        "guzzle/guzzle": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "*",
        "php-coveralls/php-coveralls": "2.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {}
    },
    "autoload": {
        "psr-4": {
            "Aboodma\\GosellSdkPhp8\\": "src/"
        },
        "classmap": ["src/GoSell.php", "src/Customers.php", "src/Authorize.php", "src/Charges.php", "src/Refunds.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "TAboodma\\GosellSdkPhp8\\": ["tests/", "tests/TapPayments/"]
        }
    },
    "authors": [
        {
            "name": "Abdulrahman Mardinli",
            "email": "34406204+aboodma@users.noreply.github.com"
        }
    ]
}
